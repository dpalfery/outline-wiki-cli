{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch CLI",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/Outlinectl.Cli/bin/Debug/net10.0/Outlinectl.Cli.dll",
      "args": [],
      "cwd": "${workspaceFolder}/Outlinectl.Cli",
      "console": "integratedTerminal"
    },
    {
      "name": "Attach to running CLI",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}